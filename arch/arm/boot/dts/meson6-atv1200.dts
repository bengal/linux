/dts-v1/;
/include/ "meson6.dtsi"

/ {
	model = "Geniatech ATV1200";
	compatible = "geniatech,atv1200";

	memory {
		reg = <0x40000000 0x80000000>;
	};

	soc@c8100000 {
		uart_AO: serial@c81004c0 {
			status = "okay";
		};
	};
}; /* end of / */

